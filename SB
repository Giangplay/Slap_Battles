	Name = "Auto spam Zero G Sound",
	Default = false,
	Callback = function(bool)
		 
        spamZeroG = bool
        
        if bool == true then
            
            while spamZeroG do
                
                task.wait()
                
                game:GetService("ReplicatedStorage").ZeroGSound:FireServer()
                
            end
        end
	end    
})


