Tab12:AddToggle({
	Name = "godmode [ Glove Golden ] [ PE ]",
	Default = false,
	Callback = function(Value)
		if Value then
	local args = {
        [1] = true
    }
    
    game:GetService("ReplicatedStorage").Goldify:FireServer(unpack(args))  
    else
	local args = {
        [1] = false,
        [2] = BrickColor.new(1003)
    }
    
    game:GetService("ReplicatedStorage").Goldify:FireServer(unpack(args))
    end
	end    
})

Tab12:AddToggle({
	Name = "Reverse godmode [ Glove Reverse ]",
	Default = false,
	Callback = function(bool)
		
	    autoReverse = bool
	    
	    if bool == true then
		
	    while autoReverse do
	       
	    task.wait()
	    
	    local Character = workspace:WaitForChild(game.Players.LocalPlayer.Name)
	    
	    if game.Players.LocalPlayer.leaderstats.Glove.Value == "Reverse" and Character:FindFirstChild("entered") then
	        
            task.wait(5.7)
            
            game:GetService("ReplicatedStorage"):WaitForChild("ReverseAbility"):FireServer()

        
	        
	    end

	    end
	    
        end
	end    
})

Tab12:AddButton({
	Name = "Inf Jump",
	Callback = function()
      		local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)
  	end    
})

Tab12:AddButton({
	Name = "Glove make block [ Default ]",
	Callback = function()
      		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
if v:IsA("Tool") then
if v.Glove.Mesh then
v.Glove.Mesh:Destroy()
end
end
end
  	end    
})

Tab14:AddToggle({
	Name = "Auto Spam Error [ no glove ]",
	Default = false,
	Callback = function(bool)
		   
        errorSpam = bool
        
        if bool == true then
            
            while errorSpam do
                
                task.wait()
                
                game.ReplicatedStorage.ErrorDeath:FireServer()
                
            end
        end
	end    
})

Tab14:AddToggle({
	Name = "Auto Spam thanos [ glove thanos ]",
	Default = false,
	Callback = function(bool)
		  
        autoThanos = bool
        
        if bool == true then
            
            while autoThanos do
                
                task.wait()

                if getGlove() == "Thanos" then
    
                    task.wait()
                    
                    game.ReplicatedStorage.Illbeback:FireServer()
                
                end
            end
    
        end
	end    
})

Tab14:AddToggle({
	Name = "Auto Spam sleep [ glove ZZZZZZZ ]",
	Default = false,
	Callback = function(bool)
		   
        spamSleep = bool
        
        if bool == true then
            
            while spamSleep do
                
                task.wait()
                
                if getGlove() == "ZZZZZZZ" then
                    
                    task.wait(.1)
                    
                    game:GetService("ReplicatedStorage").ZZZZZZZSleep:FireServer()
                    
                end
                
                
            end
            
        end
	end    
})

Tab14:AddToggle({
	Name = "Auto Invis [ glove Ghost ]",
	Default = false,
	Callback = function(bool)
		    
        spamInvis = bool
        
        if bool == true then
            
            while spamInvis do
                
                task.wait()
                
                if getGlove() == "Ghost" then
                    
                    task.wait()
                    
                    game.ReplicatedStorage.Ghostinvisibilityactivated:FireServer()
                    game.ReplicatedStorage.Ghostinvisibilitydeactivated:FireServer()
                    
                end
                
                
            end
            
        end
	end    
})

