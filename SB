Tab16:AddButton({
	Name = "Copy Anakin#3568",
	Callback = function()
      		
        if setclipboard then
            
            setclipboard("Anakin#3568")
            
        end
  	end    
})

Tab16:AddButton({
	Name = "Copy Join Discord R2O",
	Callback = function()
	
      	if setclipboard then
            
            setclipboard("https://discord.gg/zty372wma5")
            
        end
  	end    
})

Tab16:AddButton({
	Name = "Destroy GUI",
	Callback = function()
      		OrionLib:Destroy()
  	end    
})

OrionLib:Init()
